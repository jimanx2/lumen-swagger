{
    "name": "jimanx2/lumen-swagger",
    "description": "Swagger yaml generator",
    "keywords": ["api", "swagger", "open auth"],
    "license": "MIT",
    "version": "1.4.6",
    "authors": [
        {
            "name": "Digit",
            "email": "digit.vova@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "symfony/yaml": "^6.1",
        "illuminate/support": "^8.0|^9.0",
        "illuminate/filesystem": "^8.0|^9.0",
        "illuminate/routing": "^8.0|^9.0",
        "illuminate/console": "^8.0|^9.0",
        "phpdocumentor/reflection-docblock": "^4.3|^5.0",
        "doctrine/annotations": "^1.6"
    },
    "autoload": {
        "psr-4": {
            "Jimanx2\\LumenSwaggerGenerator\\": "src/",
            "OA\\": "oa/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jimanx2\\LumenSwaggerGenerator\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jimanx2\\LumenSwaggerGenerator\\SwaggerGeneratorServiceProvider"
            ]
        }
    }
}
